<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="pallet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/vehicle.xsd">
    <parentFile xmlFilename="$data/objects/pallets/butterBoxPallet/butterBoxPallet.xml">
        <attributes>
            <set path="vehicle.storeData.name" value="$l10n_fillType_butter"/>
            <set path="vehicle.storeData.image" value="textures/store_butterBoxPallet.png"/>
            <set path="vehicle.storeData.price" value="1075"/>
            <set path="vehicle.storeData.showInStore" value="true"/>
            <set path="vehicle.fillUnit.fillUnitConfigurations.fillUnitConfiguration(0).fillUnits.fillUnit#fillTypes" value="BUTTER"/>
            <set path="vehicle.fillUnit.fillUnitConfigurations.fillUnitConfiguration(0).fillUnits.fillUnit#startFillType" value="BUTTER"/>
            <set path="vehicle.fillUnit.fillUnitConfigurations.fillUnitConfiguration(0).fillUnits.fillUnit#capacity" value="1000"/>
            <set path="vehicle.fillUnit.fillUnitConfigurations.fillUnitConfiguration(0).fillUnits.fillUnit#startFillLevel" value="1000"/>
            <set path="vehicle.dischargeable.dischargeNode(0)#emptySpeed" value="400"/>
        </attributes>
    </parentFile>

    <!-- ONLY FOR ICON GENERATION -->
    <storeData>
        <name>$l10n_fillType_butter</name>
        <image>textures/store_butterBoxPallet.png</image>
        <brand>LIZARD</brand>
        <category>pallets</category>
        <vertexBufferMemoryUsage>0</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>0</indexBufferMemoryUsage>
        <textureMemoryUsage>0</textureMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>
</vehicle>
