<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="pallet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/vehicle.xsd">
    <parentFile xmlFilename="xml/pallets/woodPallet.xml">
        <attributes>
            <set path="vehicle.storeData.showInStore" value="true"/>
            <!-- <set path="vehicle.storeData.specs.weight#ignore" value="true"/> -->
            <!-- <set path="vehicle.base.components.component(0)#mass" value="100"/> -->
            <!-- <set path="vehicle.fillUnit.fillUnitConfigurations.fillUnitConfiguration(0).fillUnits.fillUnit#updateMass" value="false"/> -->
            <set path="vehicle.fillUnit.fillUnitConfigurations.fillUnitConfiguration(0).fillUnits.fillUnit#startFillType" value="WOOD"/>
            <set path="vehicle.fillUnit.fillUnitConfigurations.fillUnitConfiguration(0).fillUnits.fillUnit#startFillLevel" value="1000"/>
        </attributes>
    </parentFile>

    <!-- ONLY FOR ICON GENERATION -->
    <storeData>
        <name>$l10n_fillType_wool</name>
        <image>$data/objects/pallets/boardsStackPallet/boardsStackPallet.png</image>
        <brand>LIZARD</brand>
        <category>pallets</category>
        <vertexBufferMemoryUsage>0</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>0</indexBufferMemoryUsage>
        <textureMemoryUsage>0</textureMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>
</vehicle>
