<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="pallet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../bin/shared/xml/schema/vehicle.xsd">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>

    <storeData>
        <name>$l10n_storeItem_catTreePallet</name>
        <image>$data/store/store_empty.png</image>
        <category>pallets</category>
        <brand>LIZARD</brand>
        <showInStore>false</showInStore>
        <canBeSold>false</canBeSold>
        <financeCategory>OTHER</financeCategory>
        <vertexBufferMemoryUsage>0</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>0</indexBufferMemoryUsage>
        <textureMemoryUsage>0</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_pallet</typeDesc>
        <filename>xml/pallets/overrides/catTreePallet/catTreePallet.i3d</filename>
        <size width="1.3" length="0.9" height="1.0" lengthOffset="0" />
        <canBeReset>false</canBeReset>
        <components maxMass="600">
            <component centerOfMass="0 0.2 0" solverIterationCount="10" mass="500"/>
        </components>
        <showInVehicleMenu>false</showInVehicleMenu>
        <mapHotspot available="false" />
        <input allowed="false"/>
    </base>

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits removeVehicleIfEmpty="true">
                    <fillUnit unitTextOverride="$l10n_unit_pieces" fillTypes="CATTREE" capacity="1000" ignoreFillLimit="true"/>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <dischargeable requiresTipOcclusionArea="false">
        <dischargeNode node="dischargeNode" emptySpeed="500" fillUnitIndex="1" maxDistance="6" canStartDischargeAutomatically="true" playSound="false"/>
    </dischargeable>

    <pallet fillUnitIndex="1" node="catTreePallet_vis">
        <content>
            <object node="catTree01" tensionBeltNode="catTree01Tension"/>
            <object node="catTree02" tensionBeltNode="catTree02Tension"/>
            <object node="catTree03" tensionBeltNode="catTree03Tension"/>
            <object node="catTree04" tensionBeltNode="catTree04Tension"/>
            <object node="catTree05" tensionBeltNode="catTree05Tension"/>
            <object node="catTree06" tensionBeltNode="catTree06Tension"/>
            <object node="catTree07" tensionBeltNode="catTree07Tension"/>
            <object node="catTree08" tensionBeltNode="catTree08Tension"/>
            <object node="catTree09" tensionBeltNode="catTree09Tension"/>
            <object node="catTree10" tensionBeltNode="catTree10Tension"/>
            <object node="catTree11" tensionBeltNode="catTree11Tension"/>
            <object node="catTree12" tensionBeltNode="catTree12Tension"/>
        </content>
        <sounds>
            <unload template="palletsFillWood" pitchScale="1" volumeScale="1.5" supportsReverb="false"/>
        </sounds>
    </pallet>

    <dynamicMount triggerNode="dynamicMountTrigger" triggerForceAcceleration="4" additionalMountDistance="0.969" jointTransY="0.106" jointLimitToRotY="true"/>

    <i3dMappings>
        <i3dMapping id="catTreePallet_main_component1" node="0>" />
        <i3dMapping id="catTreePallet_vis" node="0>0" />
        <i3dMapping id="catTree01" node="0>0|0|0" />
        <i3dMapping id="catTree01Tension" node="0>0|0|0|0" />
        <i3dMapping id="catTree02" node="0>0|0|1" />
        <i3dMapping id="catTree02Tension" node="0>0|0|1|0" />
        <i3dMapping id="catTree03" node="0>0|0|2" />
        <i3dMapping id="catTree03Tension" node="0>0|0|2|0" />
        <i3dMapping id="catTree04" node="0>0|0|3" />
        <i3dMapping id="catTree04Tension" node="0>0|0|3|0" />
        <i3dMapping id="catTree05" node="0>0|0|4" />
        <i3dMapping id="catTree05Tension" node="0>0|0|4|0" />
        <i3dMapping id="catTree06" node="0>0|0|5" />
        <i3dMapping id="catTree06Tension" node="0>0|0|5|0" />
        <i3dMapping id="catTree07" node="0>0|0|6" />
        <i3dMapping id="catTree07Tension" node="0>0|0|6|0" />
        <i3dMapping id="catTree08" node="0>0|0|7" />
        <i3dMapping id="catTree08Tension" node="0>0|0|7|0" />
        <i3dMapping id="catTree09" node="0>0|0|8" />
        <i3dMapping id="catTree09Tension" node="0>0|0|8|0" />
        <i3dMapping id="catTree10" node="0>0|0|9" />
        <i3dMapping id="catTree10Tension" node="0>0|0|9|0" />
        <i3dMapping id="catTree11" node="0>0|0|10" />
        <i3dMapping id="catTree11Tension" node="0>0|0|10|0" />
        <i3dMapping id="catTree12" node="0>0|0|11" />
        <i3dMapping id="catTree12Tension" node="0>0|0|11|0" />
        <i3dMapping id="dynamicMountTrigger" node="0>0|1" />
        <i3dMapping id="dischargeNode" node="0>0|2" />
    </i3dMappings>
</vehicle>
