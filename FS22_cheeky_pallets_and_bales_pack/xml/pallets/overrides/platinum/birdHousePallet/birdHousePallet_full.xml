<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="pallet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../bin/shared/xml/schema/vehicle.xsd">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>

    <storeData>
        <name>$l10n_storeItem_birdHousePallet</name>
        <image>$data/store/store_empty.png</image>
        <category>pallets</category>
        <brand>LIZARD</brand>
        <showInStore>false</showInStore>
        <canBeSold>false</canBeSold>
        <financeCategory>OTHER</financeCategory>
        <vertexBufferMemoryUsage>0</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>0</indexBufferMemoryUsage>
        <textureMemoryUsage>0</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_pallet</typeDesc>
        <filename>xml/pallets/overrides/birdHousePallet/birdHousePallet.i3d</filename>
        <size width="1.3" length="0.9" height="0.6" lengthOffset="0" />
        <canBeReset>false</canBeReset>
        <components maxMass="600">
            <component centerOfMass="0 0.2 0" solverIterationCount="10" mass="500"/>
        </components>
        <showInVehicleMenu>false</showInVehicleMenu>
        <mapHotspot available="false" />
        <input allowed="false"/>
    </base>

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits removeVehicleIfEmpty="true">
                    <fillUnit unitTextOverride="$l10n_unit_pieces" fillTypes="BIRDHOUSE" capacity="1000" ignoreFillLimit="true"/>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <dischargeable requiresTipOcclusionArea="false">
        <dischargeNode node="dischargeNode" emptySpeed="500" fillUnitIndex="1" maxDistance="6" canStartDischargeAutomatically="true" playSound="false"/>
    </dischargeable>

    <pallet fillUnitIndex="1" node="birdHousePallet_vis" >
        <content>
            <object node="birdHouse01" tensionBeltNode="birdHouse01Tension"/>
            <object node="birdHouse02" tensionBeltNode="birdHouse02Tension"/>
            <object node="birdHouse03" tensionBeltNode="birdHouse03Tension"/>
            <object node="birdHouse04" tensionBeltNode="birdHouse04Tension"/>
            <object node="birdHouse05" tensionBeltNode="birdHouse05Tension"/>
            <object node="birdHouse06" tensionBeltNode="birdHouse06Tension"/>
            <object node="birdHouse07" tensionBeltNode="birdHouse07Tension"/>
        </content>
        <sounds>
            <unload template="palletsFillWood" pitchScale="1" volumeScale="2.5" supportsReverb="false"/>
        </sounds>
    </pallet>

    <dynamicMount triggerNode="dynamicMountTrigger" triggerForceAcceleration="4" additionalMountDistance="0.554" jointTransY="0.106" jointLimitToRotY="true"/>

    <i3dMappings>
        <i3dMapping id="birdHousePallet_main_component1" node="0>" />
        <i3dMapping id="birdHousePallet_vis" node="0>0" />
        <i3dMapping id="birdHouse01" node="0>0|0|0" />
        <i3dMapping id="birdHouse01Tension" node="0>0|0|0|0" />
        <i3dMapping id="birdHouse02" node="0>0|0|1" />
        <i3dMapping id="birdHouse02Tension" node="0>0|0|1|0" />
        <i3dMapping id="birdHouse03" node="0>0|0|2" />
        <i3dMapping id="birdHouse03Tension" node="0>0|0|2|0" />
        <i3dMapping id="birdHouse04" node="0>0|0|3" />
        <i3dMapping id="birdHouse04Tension" node="0>0|0|3|0" />
        <i3dMapping id="birdHouse05" node="0>0|0|4" />
        <i3dMapping id="birdHouse05Tension" node="0>0|0|4|0" />
        <i3dMapping id="birdHouse06" node="0>0|0|5" />
        <i3dMapping id="birdHouse06Tension" node="0>0|0|5|0" />
        <i3dMapping id="birdHouse07" node="0>0|0|6" />
        <i3dMapping id="birdHouse07Tension" node="0>0|0|6|0" />
        <i3dMapping id="dynamicMountTrigger" node="0>0|1" />
        <i3dMapping id="dischargeNode" node="0>0|2" />
    </i3dMappings>
</vehicle>
