<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="pallet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../bin/shared/xml/schema/vehicle.xsd">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>

    <storeData>
        <name>$l10n_storeItem_staircaseRailingPallet</name>
        <image>$data/store/store_empty.png</image>
        <category>pallets</category>
        <brand>LIZARD</brand>
        <showInStore>false</showInStore>
        <canBeSold>false</canBeSold>
        <financeCategory>OTHER</financeCategory>
        <vertexBufferMemoryUsage>0</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>0</indexBufferMemoryUsage>
        <textureMemoryUsage>0</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_pallet</typeDesc>
        <filename>xml/pallets/overrides/staircaseRailingPallet/staircaseRailingPallet.i3d</filename>
        <size width="1.3" length="0.9" height="0.9" lengthOffset="0" />
        <canBeReset>false</canBeReset>
        <components maxMass="600">
            <component centerOfMass="0 0.2 0" solverIterationCount="10" mass="400"/>
        </components>
        <showInVehicleMenu>false</showInVehicleMenu>
        <mapHotspot available="false" />
        <input allowed="false"/>
    </base>

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits removeVehicleIfEmpty="true">
                    <fillUnit unitTextOverride="$l10n_unit_pieces" fillTypes="STAIRCASERAILING" capacity="1000" ignoreFillLimit="true"/>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <dischargeable requiresTipOcclusionArea="false">
        <dischargeNode node="dischargeNode" emptySpeed="500" fillUnitIndex="1" maxDistance="6" canStartDischargeAutomatically="true" playSound="false"/>
    </dischargeable>

    <pallet fillUnitIndex="1" node="staircaseRailingPallet_vis">
        <content>
            <object node="staircase01" tensionBeltNode="staircase01Tension"/>
            <object node="staircase02" tensionBeltNode="staircase02Tension"/>
            <object node="staircase03" tensionBeltNode="staircase03Tension"/>
            <object node="staircase04" tensionBeltNode="staircase04Tension"/>
            <object node="staircase05" tensionBeltNode="staircase05Tension"/>
            <object node="staircase06" tensionBeltNode="staircase06Tension"/>
            <object node="staircase07" tensionBeltNode="staircase07Tension"/>
            <object node="staircase08" tensionBeltNode="staircase08Tension"/>
        </content>
        <sounds>
            <unload template="palletsFillWood" pitchScale="0.7" volumeScale="2.5" supportsReverb="false"/>
        </sounds>
    </pallet>

    <dynamicMount triggerNode="dynamicMountTrigger" triggerForceAcceleration="4" additionalMountDistance="0.82" jointTransY="0.106" jointLimitToRotY="true"/>

    <i3dMappings>
        <i3dMapping id="staircaseRailingPallet_component1" node="0>" />
        <i3dMapping id="staircaseRailingPallet_vis" node="0>0" />
        <i3dMapping id="staircase01" node="0>0|0|0" />
        <i3dMapping id="staircase01Tension" node="0>0|0|0|0" />
        <i3dMapping id="staircase02" node="0>0|0|1" />
        <i3dMapping id="staircase02Tension" node="0>0|0|1|0" />
        <i3dMapping id="staircase03" node="0>0|0|2" />
        <i3dMapping id="staircase03Tension" node="0>0|0|2|0" />
        <i3dMapping id="staircase04" node="0>0|0|3" />
        <i3dMapping id="staircase04Tension" node="0>0|0|3|0" />
        <i3dMapping id="staircase05" node="0>0|0|4" />
        <i3dMapping id="staircase05Tension" node="0>0|0|4|0" />
        <i3dMapping id="staircase06" node="0>0|0|5" />
        <i3dMapping id="staircase06Tension" node="0>0|0|5|0" />
        <i3dMapping id="staircase07" node="0>0|0|6" />
        <i3dMapping id="staircase07Tension" node="0>0|0|6|0" />
        <i3dMapping id="staircase08" node="0>0|0|7" />
        <i3dMapping id="staircase08Tension" node="0>0|0|7|0" />
        <i3dMapping id="dynamicMountTrigger" node="0>0|1" />
        <i3dMapping id="dischargeNode" node="0>0|2" />
        <i3dMapping id="collisions" node="0>0|3" />
    </i3dMappings>
</vehicle>
