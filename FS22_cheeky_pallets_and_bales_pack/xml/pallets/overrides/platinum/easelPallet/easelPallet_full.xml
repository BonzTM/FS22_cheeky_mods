<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="pallet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../bin/shared/xml/schema/vehicle.xsd">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>

    <storeData>
        <name>$l10n_storeItem_easelPallet</name>
        <image>$data/store/store_empty.png</image>
        <category>pallets</category>
        <brand>LIZARD</brand>
        <showInStore>false</showInStore>
        <canBeSold>false</canBeSold>
        <financeCategory>OTHER</financeCategory>
        <vertexBufferMemoryUsage>0</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>0</indexBufferMemoryUsage>
        <textureMemoryUsage>0</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_pallet</typeDesc>
        <filename>xml/pallets/overrides/easelPallet/easelPallet.i3d</filename>
        <size width="1.3" length="0.9" height="0.7" yRotation="0" />
        <canBeReset>false</canBeReset>
        <components maxMass="600">
            <component centerOfMass="0 0.2 0" solverIterationCount="10" mass="600"/>
        </components>
        <showInVehicleMenu>false</showInVehicleMenu>
        <mapHotspot available="false" />
        <input allowed="false"/>
    </base>

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits removeVehicleIfEmpty="true">
                    <fillUnit unitTextOverride="$l10n_unit_pieces" fillTypes="EASEL" capacity="1000" ignoreFillLimit="true"/>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <dischargeable requiresTipOcclusionArea="false">
        <dischargeNode node="dischargeNode" emptySpeed="500" fillUnitIndex="1" maxDistance="6" canStartDischargeAutomatically="true" playSound="false"/>
    </dischargeable>

    <pallet fillUnitIndex="1" node="easelPallet_vis">
        <content>
            <object node="easel01" tensionBeltNode="easel01Tension"/>
            <object node="easel02" tensionBeltNode="easel02Tension"/>
            <object node="easel03" tensionBeltNode="easel03Tension"/>
            <object node="easel04" tensionBeltNode="easel04Tension"/>
            <object node="easel05" tensionBeltNode="easel05Tension"/>
            <object node="easel06" tensionBeltNode="easel06Tension"/>
            <object node="easel07" tensionBeltNode="easel07Tension"/>
            <object node="easel08" tensionBeltNode="easel08Tension"/>
            <object node="easel09" tensionBeltNode="easel09Tension"/>
            <object node="easel10" tensionBeltNode="easel10Tension"/>
            <object node="easel11" tensionBeltNode="easel11Tension"/>
            <object node="easel12" tensionBeltNode="easel12Tension"/>
        </content>
        <sounds>
            <unload template="palletsFillWood" pitchScale="1" volumeScale="1.7" supportsReverb="false"/>
        </sounds>
    </pallet>

    <dynamicMount triggerNode="dynamicMountTrigger" triggerForceAcceleration="4" additionalMountDistance="0.689" jointTransY="0.106" jointLimitToRotY="true"/>

    <i3dMappings>
        <i3dMapping id="easelPallet_main_component1" node="0>" />
        <i3dMapping id="easelPallet_vis" node="0>0" />
        <i3dMapping id="easel01" node="0>0|0|0" />
        <i3dMapping id="easel01Tension" node="0>0|0|0|0" />
        <i3dMapping id="easel02" node="0>0|0|1" />
        <i3dMapping id="easel02Tension" node="0>0|0|1|0" />
        <i3dMapping id="easel03" node="0>0|0|2" />
        <i3dMapping id="easel03Tension" node="0>0|0|2|0" />
        <i3dMapping id="easel04" node="0>0|0|3" />
        <i3dMapping id="easel04Tension" node="0>0|0|3|0" />
        <i3dMapping id="easel05" node="0>0|0|4" />
        <i3dMapping id="easel05Tension" node="0>0|0|4|0" />
        <i3dMapping id="easel06" node="0>0|0|5" />
        <i3dMapping id="easel06Tension" node="0>0|0|5|0" />
        <i3dMapping id="easel07" node="0>0|0|6" />
        <i3dMapping id="easel07Tension" node="0>0|0|6|0" />
        <i3dMapping id="easel08" node="0>0|0|7" />
        <i3dMapping id="easel08Tension" node="0>0|0|7|0" />
        <i3dMapping id="easel09" node="0>0|0|8" />
        <i3dMapping id="easel09Tension" node="0>0|0|8|0" />
        <i3dMapping id="easel10" node="0>0|0|9" />
        <i3dMapping id="easel10Tension" node="0>0|0|9|0" />
        <i3dMapping id="easel11" node="0>0|0|10" />
        <i3dMapping id="easel11Tension" node="0>0|0|10|0" />
        <i3dMapping id="easel12" node="0>0|0|11" />
        <i3dMapping id="easel12Tension" node="0>0|0|11|0" />
        <i3dMapping id="dynamicMountTrigger" node="0>0|1" />
        <i3dMapping id="dischargeNode" node="0>0|2" />
    </i3dMappings>
</vehicle>
