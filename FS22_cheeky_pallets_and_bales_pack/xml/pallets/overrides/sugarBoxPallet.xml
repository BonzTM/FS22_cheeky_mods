            <object node="sugarBox10" tensionBeltNode="sugarBox10Tension"/>
            <object node="sugarBox11" tensionBeltNode="sugarBox11Tension"/>
            <object node="sugarBox12" tensionBeltNode="sugarBox12Tension"/>
        </content>

        <sounds>
            <unload template="palletsFillSugar" pitchScale="0.9" />
        </sounds>
    </pallet>

    <dynamicMount triggerNode="dynamicMountTrigger" triggerForceAcceleration="4" additionalMountDistance="1.1" jointTransY="0.11" jointLimitToRotY="true"/>

