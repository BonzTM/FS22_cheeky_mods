<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<vehicle type="pallet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../shared/xml/schema/vehicle.xsd">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>

    <storeData>
        <name>$l10n_storeItem_honeyBoxPallet</name>
        <image>$data/store/store_empty.png</image>
        <category>pallets</category>
        <brand>LIZARD</brand>
        <showInStore>false</showInStore>
        <canBeSold>false</canBeSold>
        <financeCategory>OTHER</financeCategory>
        <vertexBufferMemoryUsage>105472</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>15872</indexBufferMemoryUsage>
        <textureMemoryUsage>1835008</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_pallet</typeDesc>
        <filename>$data/objects/pallets/honeyBoxPallet/honeyBoxPallet.i3d</filename>
        <size width="1.4" length="1" height="0.7" />
        <canBeReset>false</canBeReset>
        <components>
            <component centerOfMass="0 0.1 0" solverIterationCount="10" mass="60"/>
        </components>
        <showInVehicleMenu>false</showInVehicleMenu>
        <mapHotspot available="false" />
        <input allowed="false"/>
    </base>

    <fillUnit>
        <fillUnitConfigurations>
            <fillUnitConfiguration>
                <fillUnits removeVehicleIfEmpty="true">
                    <fillUnit unitTextOverride="$l10n_unit_literShort" fillTypes="HONEY" capacity="400"/>
                </fillUnits>
            </fillUnitConfiguration>
        </fillUnitConfigurations>
    </fillUnit>

    <dischargeable requiresTipOcclusionArea="false">
        <dischargeNode node="dischargeNode" emptySpeed="200" fillUnitIndex="1" maxDistance="6" canStartDischargeAutomatically="true" playSound="false"/>
    </dischargeable>

    <pallet fillUnitIndex="1" node="pallet_vis">
        <content>
            <object node="honeyBox1" tensionBeltNode="honeyBox1Tension"/>
            <object node="honeyBox2" tensionBeltNode="honeyBox2Tension"/>
            <object node="honeyBox3" tensionBeltNode="honeyBox3Tension"/>
            <object node="honeyBox4" tensionBeltNode="honeyBox4Tension"/>
            <object node="honeyBox5" tensionBeltNode="honeyBox5Tension"/>
            <object node="honeyBox6" tensionBeltNode="honeyBox6Tension"/>
            <object node="honeyBox7" tensionBeltNode="honeyBox7Tension"/>
            <object node="honeyBox8" tensionBeltNode="honeyBox8Tension"/>
        </content>

        <sounds>
            <unload template="palletsFillWood" pitchScale="0.8" />
        </sounds>
    </pallet>

    <dynamicMount triggerNode="dynamicMountTrigger" triggerForceAcceleration="4" additionalMountDistance="0.6" jointTransY="0.11" jointLimitToRotY="true"/>

    <i3dMappings>
        <i3dMapping id="honeyBoxPallet_component1" node="0>" />
        <i3dMapping id="pallet_vis" node="0>0" />
        <i3dMapping id="dynamicMountTrigger" node="0>0|0" />
        <i3dMapping id="dischargeNode" node="0>0|1" />
        <i3dMapping id="honeyBox1" node="0>0|2|0" />
        <i3dMapping id="honeyBox1Tension" node="0>0|2|0|0" />
        <i3dMapping id="honeyBox2" node="0>0|2|1" />
        <i3dMapping id="honeyBox2Tension" node="0>0|2|1|0" />
        <i3dMapping id="honeyBox3" node="0>0|2|2" />
        <i3dMapping id="honeyBox3Tension" node="0>0|2|2|0" />
        <i3dMapping id="honeyBox4" node="0>0|2|3" />
        <i3dMapping id="honeyBox4Tension" node="0>0|2|3|0" />
        <i3dMapping id="honeyBox5" node="0>0|2|4" />
        <i3dMapping id="honeyBox5Tension" node="0>0|2|4|0" />
        <i3dMapping id="honeyBox6" node="0>0|2|5" />
        <i3dMapping id="honeyBox6Tension" node="0>0|2|5|0" />
        <i3dMapping id="honeyBox7" node="0>0|2|6" />
        <i3dMapping id="honeyBox7Tension" node="0>0|2|6|0" />
        <i3dMapping id="honeyBox8" node="0>0|2|7" />
        <i3dMapping id="honeyBox8Tension" node="0>0|2|7|0" />
    </i3dMappings>
</vehicle>
